{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://cdn.glitch.com/49d34dc6-8fbd-46bb-8221-b99ffd36f1af%2Ftouchicon-180.png",
          "https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/fonts/*.woff2?v=4.7.0",
          "https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/*.css",
          "https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/*.css",
          "https://fonts.googleapis.com/css2?family=Roboto:wght@400&display=swap"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "employees",
      "urls": [
        "http://localhost:3000/employees",
        "https://picsum.photos/200.webp"
      ],
      "cacheConfig": {
        "maxAge": "30d",
        "maxSize": 15,
        "strategy": "freshness",
        "timeout": "6s"
      }
    }
  ]
}
