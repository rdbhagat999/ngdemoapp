{
  "name": "ngdemoapp",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "filldata": "node ./server/filldata.js > ./server/data.json",
    "server": "json-server --watch ./server/data.json",
    "mock-data-server": "npm run filldata && npm run server",
    "git-deploy": "ng deploy --base-href=/ngdemoapp/",
    "dev:ssr": "ng run ngdemoapp:serve-ssr",
    "serve:ssr": "node dist/ngdemoapp/server/main.js",
    "build:ssr": "ng build --prod && ng run ngdemoapp:server:production",
    "serve:prerender": "http-server ./dist/ngdemoapp/browser -c-1 -b -g -d",
    "prerender": "ng run ngdemoapp:prerender --routes-file prerender-routes.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.9",
    "@angular/common": "~11.2.9",
    "@angular/compiler": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@angular/forms": "~11.2.9",
    "@angular/platform-browser": "~11.2.9",
    "@angular/platform-browser-dynamic": "~11.2.9",
    "@angular/platform-server": "~11.2.9",
    "@angular/router": "~11.2.9",
    "@angular/service-worker": "~11.2.9",
    "@nguniversal/express-engine": "^11.2.1",
    "angular2-qrcode": "^2.0.3",
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "ng2-pdf-viewer": "^6.3.2",
    "ngx-clipboard": "^14.0.1",
    "ngx-skeleton-loader": "^2.6.2",
    "rxjs": "~6.6.7",
    "sweetalert2": "^10.13.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.8",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "~11.2.9",
    "@nguniversal/builders": "^11.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.0",
    "faker": "^5.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
